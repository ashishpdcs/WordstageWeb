        function LoadProductData(Product, from, to) {
            $.ajax({
                url: '/Home/LoadProductData',
                data: { 'Product': Product, 'from': from, 'to': to },
                type: 'Post',
                dataType: 'json',
                success: function (result) {
                    console.log(result);
                    TablelooandupdatePlan(result);
                },
                error: function (xhr, ajaxOptions, thrownError) {
                    alert('Error: ' + thrownError);
                }
            });
        }
        function TablelooandupdatePlan(myModelList) {

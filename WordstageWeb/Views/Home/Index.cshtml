        function createLinkWithPlanType(planType, linkId, ProductId) {
            const link = document.getElementById(linkId);

            if (planType === 'STANDARD') {
                link.href = '/Order/Order';
            } else if (planType === 'ADVANCED') {
                link.href = '/Order/Order';
            } else if (planType === 'PREMIUM') {
                link.href = '/Order/Order';
            }

            // Add a click event listener to the link with parameters
            link.addEventListener('click', function () {
                sessionStorage.setItem('ProductId', ProductId);
                // myFunction(planType, ProductId);
            });
        };
        function PriceCalculation() {
            var elements = document.querySelectorAll('[Valueattribute]');
            var attributeValues = [];
            var Price = 0;
            for (var i = 0; i < elements.length; i++) {
                var value = elements[i].getAttribute('Valueattribute');
                attributeValues.push(value);
                var newid = elements[i].getAttribute('id');

                var myRadio = document.getElementById(newid);
                if (myRadio.checked) {
                    Price = Price + parseInt(value);
                }
            }
            if (document.getElementById("notarizedtranslationbutonYes").style.background == "cornflowerblue") {
                const attributeValue = document.getElementById("notarizedtranslationbutonYes").getAttribute("Valueattribute");
                Price = Price + parseInt(attributeValue);
            }
            TotalPrice = Price;
            document.getElementById('price').innerHTML = "₹ " + Price.toFixed(2);
        }
